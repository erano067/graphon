{
  "name": "@graphon/core",
  "version": "0.1.5",
  "description": "High-performance graph visualization library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erano067/graphon.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit && tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "comlink": "^4.4.2",
    "graphology": "^0.26.0",
    "graphology-types": "^0.24.8",
    "pixi.js": "^8.15.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "vitest": "^4.0.18"
  }
}
